#!/bin/sh
cd /boot/home/projects/GraphicsWorkflow/AddBorderToImage/PackagingFolder
package create -b addbordertoimage_0.1-1_gcc2.hpkg
sleep 0.5
package add addbordertoimage_0.1-1_gcc2.hpkg data apps bin
sleep 0.5
addattr BeShare:Info "add border to images" addbordertoimage_0.1-1_gcc2.hpkg
becopy addbordertoimage_0.1-1_gcc2.hpkg "/Storage/Haiku/My ports"
mv addbordertoimage_0.1-1_gcc2.hpkg /boot/home/projects/repo2
sync
