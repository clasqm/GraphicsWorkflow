import libclasqm_main

if peek("argument")<>0 then 
	TheFile$ = peek$("argument")
else
	exit
endif

system("mogrify -flip '" + TheFile$ + "'")
system("mimeset -F '" + TheFile$ + "'")

print "'" + TheFile$ + "'"

exit


